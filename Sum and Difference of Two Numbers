#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
	int a ,b,sum,sub;
    float c,d;
    scanf("%d",&a);
    scanf("%d",&b);
    scanf("\n");
    scanf("%f%f",&c,&d);
    sum=a+b;
    sub=a-b;
    printf("%d %d\n",a+b,a-b);
    sum=c+d;
    sub=c-d;
    printf("%.1f %.1f",c+d,c-d);
    
    
    return 0;
}
